C--           replace compiler name

      CALL CCOSUB (LINE,NTX, LINEWK,1,512,
     +            TKNAME, CONAME(1:NNM))
      IF (NGSLAT.NE.0)  THEN
          NTX = NDSLAT
          LINE(1:NTX) = LINEWK(1:NTX)
