   51 IF (MOPTIO(26).NE.0)         GO TO 87
      CALL QTOPSY (KQKEEP)
      CALL QSORTN (1,KQKEEP)
      NBK = NQCOUNT (LQKEEP)

      NQUSED = MOD (NQUSED, NQLMAX)

      IF (NQUSED+NBK+4.GE.NQLMAX)  THEN
          WRITE (IQPRNT,9051)
          NQUSED = 3
        ELSE
          WRITE (IQPRNT,9052)
          NQUSED = NQUSED + 3
        ENDIF

      LZ = LQKEEP
   52 IF (LZ.EQ.0)                 GO TO 87
