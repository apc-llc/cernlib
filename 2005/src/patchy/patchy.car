      SUBROUTINE PSTEER

C-    Running Patchy
C.    started  7-feb-92

+CDE, QSTATE, Q, PY.
C--------------    End CDE              --------------------------------


      CALL PINIT (0)

C--       Get the cradle into memory

      INCRAD = 3
      CALL ARRIVE (0)

C--       Complete the initialization

      CALL PINIT (1)

C--       Do the cradle initially

      CALL DOCRAD
      IF (JANSW.NE.0)              GO TO 89

C--       Do next PAM file

   41 INCRAD = 0
      CALL DOPAM

C--       Do cradle continuation

      INCRAD = 2
      CALL DOCRAD
      IF (JANSW.EQ.0)              GO TO 41

   89 IF (NQERR.NE.0)        CALL PABEND
      CALL PEND
      RETURN
      END
