      JSLE = JSLA + NSL
      WRITE (IQPRNT,9000) NSL,JSLA,MSG

 9000 FORMAT (/' DUMPSL: dump',I5,' lines at slot',I7,', ',A/)

      JSLE = MIN (JSLE, JSLA+4000)
      JSLU = JSLA - 1
   21 JSLU = JSLU + 1
      IF (JSLU.GE.JSLE)            RETURN

      JTXA = MLIAD(JSLU)
      JTXE = MLIAD(JSLU+1)
