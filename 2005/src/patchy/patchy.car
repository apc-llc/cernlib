+CDE, QSH.
C--------------    End CDE              --------------------------------

      LL = LQ(KGO)
      IF (LL.EQ.0)           RETURN
      JW = JWORD

   11 LN = LQ(LL-1)
      IF (LN.EQ.0)           RETURN
      IF (NA_DIF(IQ(LN+JW),IQ(LL+JW)).LT.0)  GO TO 21
      LL = LN
      GO TO 11

C--                BANK  LN  OUT OF SEQUENCE

   21 LQ(LL-1)= LQ(LN-1)
      K = KGO

   24 L = LQ(K)
      IF (NA_DIF(IQ(LN+JW),IQ(L+JW)).LT.0)  GO TO 29
      K = L-1
      GO TO 24

C--                PLACE FOR BANK  LN  FOUND

   29 LQ(LN-1)= L
      LQ(K)   = LN
      GO TO 11
      END
