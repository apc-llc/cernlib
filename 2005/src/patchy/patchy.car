      CALL EXITRC (2)

 9080 FORMAT (/' ***!!! No file-name given, no execution !!!***')
 9081 FORMAT (/' ***!!! Errors on input, no execution !!!***')
 9094 FORMAT (/' **!! Zero routines to be compiled !!**')
 9095 FORMAT (/' **!! No routines to be compiled or deleted !!**')

   94 IF (MOPTIO(21).EQ.0)  THEN
          WRITE (IQPRNT,9094)
        ELSE
          WRITE (IQPRNT,9095)
