      L = LDO
      IF (JSLN.EQ.0)               GO TO 41
      GO TO 22

   21 L = LQ(L-1)
      IF (L.EQ.0)                  GO TO 49
   22 JSLA = IQ(L+1)
      JSLE = IQ(L+2) + JSLA
