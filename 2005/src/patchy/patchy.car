+CDE, CHEXC, QBANKS, QPAGE.
+CDE, Q, PY.
C--------------    End CDE              --------------------------------


      CALL MQLIFT (LEXD, LQUSER(2),1, JBKDEC,3)
      IQ(LEXD+1) = IXEXDEC
      IQ(LEXD+2) = IXEXPAT
      IQ(LEXD+3) = NQDKNO
      RETURN
      END
