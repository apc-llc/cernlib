+SEQ, QEJECT.
      CHARACTER*(*)  CHEXTF, CHEXTC, CHEXTI, CHEXTA, CHEXTD, CHEXTS
+SELF, IF=QF_APO.
      PARAMETER    (CHEXTF= '.ftn')
+SELF, IF=QS_IBMVM.
      PARAMETER    (CHEXTF= '.fortran')
+SELF, IF=QS_VMS.
      PARAMETER    (CHEXTF= '.for')
