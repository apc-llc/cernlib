      PARAMETER    (CHEXTS= '.exec')
+SELF, IF=QS_VMS.
      PARAMETER    (CHEXTS= '.com')
+SELF, IF=-QS_IBMVM, IF=-QS_VMS.
      PARAMETER    (CHEXTS= '.sh')
+SELF.


      N_TYP   = 0
      NAL_TYP = 2

      JAL = MAX_TYP - NAL_TYP

      CH_TYP(JAL+1) = 'R*EPEAT'
      CH_TYP(JAL+2) = 'J*OIN'
      JU_TYP(JAL+1) = -1
      JU_TYP(JAL+2) = -2

C--       register routine header masks
+SELF, IF=INITRH.
      IXRHF = NA_LONG (CHRHF)
      IXRHC = NA_LONG (CHRHC)
      IXRHI = NA_LONG (CHRHI)
      IXRHA = NA_LONG (CHRHA)
