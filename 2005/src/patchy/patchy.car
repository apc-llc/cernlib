      SUBROUTINE DDPREP

C-    Dump the structure of PREP banks
C.    started 27-apr-95

+CDE, MQCN, QUNIT.
+CDE, KQADR, Q, PY.
+CDE, M_ANAC.
C--------------    End CDE              --------------------------------


      WRITE (IQPRNT,9000)
 9000 FORMAT (/' Dump the PREP structure')

      LDOAN  = KQPREP + 1
      LUPAN  = 0

C------            Next PREP bank

   22 KDOAN = LDOAN - 1
      LDOAN = LQ(KDOAN)
      IF (LDOAN.EQ.0)              RETURN

      CALL QNAMEX (LDOAN)
      IF (IQFOUL.NE.0)             GO TO 91
      CALL DQBANK
      IF (IQFOUL.NE.0)             GO TO 97

      L = LQ(LDOAN-2)
      IF (L.EQ.0)                  GO TO 22
      IF (L.EQ.-1)                 GO TO 22

      CALL QNAMEX (L)
      IF (IQFOUL.NE.0)             GO TO 92
      CALL DQBANK
      IF (IQFOUL.NE.0)             GO TO 97

C----              Follow reference to foreign material

   31 LOWAN = LDOAN
      LUPAN = L
      LDOAN = LUPAN - 1

   34 KDOAN = LDOAN - 1
      LDOAN = LQ(KDOAN)
      IF (LDOAN.EQ.0)  THEN
C--       end of chain of MAT banks
          LDOAN = LOWAN
          LUPAN = 0
          GO TO 22
        ENDIF

      CALL QNAMEX (LDOAN)
      IF (IQFOUL.NE.0)             GO TO 91
      CALL DQBANK
      IF (IQFOUL.NE.0)             GO TO 97
      GO TO 34

