C--       Pre-lift the banks for I/O handling

      CALL MQLIFT (LACRAD, 0,7, JBKARR,3)
      CALL MQLIFT (LPAM,   0,7, JBKPAM,3)
      CALL MQLIFT (LQARRV, 0,7, JBKARR,3)
      CALL MQLIFT (LQHOLD, 0,7, JBKHOL,1)
