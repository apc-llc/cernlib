      CALL SMCIL (-1, 'as:1'  , '${AS} -o $*.o ${s}$*.s')

+SELF, IF=QMVAO.
      CALL SMCIL (-1, 'fo:2'  , '-O0')
      CALL SMCIL (-1, 'fopt'  , '-c -w1 -warn nouncalled')
      CALL SMCIL (-1, 'co:1'  , '-O2')
      CALL SMCIL (-1, 'co:2'  , '-O0')
      CALL SMCIL (-1, 'as:1'  , '${AS} -o $*.o ${s}$*.s')

+SELF, IF=QMUUX.

+SELF.
      RETURN
      END
