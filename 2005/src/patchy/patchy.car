      SUBROUTINE INDCRZ (IXNAME)

C-    Register sequence name IXNAME for nyindex
C.    started 17-jan-94

+CDE, QBANKS.
+CDE, Q, PY.
C--------------    End CDE              --------------------------------
+SEQ, Q_OR.


      CALL MQLIFT (LKEE, LQKEEP,1, JBKKEE,3)
      IQ(LKEE+1) = IXNAME
      IF (LEXD.NE.0)  THEN
          LQ(LKEE-2) = LEXD
          RETURN
        ENDIF

      LQ(LKEE-2) = LEXP
      IQ(LKEE)   = IOR (IQ(LKEE),1)
      RETURN
