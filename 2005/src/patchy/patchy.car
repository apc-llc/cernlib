      CALL NA_GET (IXDEC,LINE,14)
      NN = NESLAT
      NN = MAX (NN,26)

      LINE(NN:NN) = '.'
      CALL NA_GET (IXPAT,LINE,NN+1)
      NN = NESLAT - 1

      WRITE (IQPRNT,9008) LINE(1:NN)
      NQUSED = NQUSED + 1
      LD = LQ(LD-1)
      GO TO 46

+SEQ, QEJECT.
