+DECK, TOGARB.
      SUBROUTINE TOGARB (KDO,IFLAG)

C-    Shunt the single bank connected at KDO (if IFLAG.EQ.0)
C-    or the linear structure at KDO (if IFLAG.NE.0)
C-    to the garbage structure
C.    started  5-dec-91

+CDE, MQCM, Q, PY.
C--------------    End CDE              --------------------------------


      LDO = LQ(KDO)
      IF (LDO.EQ.0)                RETURN

C--       single bank

      IF (IFLAG.NE.0)              GO TO 21
      LQ(KDO) = LQ(LDO-1)
      IF (LDO.GE.LQCSTA(2))        RETURN

      LQ(LDO-1) = LQGARB
      LQGARB    = LDO
      RETURN

C--       linear structure

   21 KL = KDO
   22 LL = LQ(KL)
      IF (LL.GT.0)         THEN
          IF (LL.GE.LQCSTA(2))  THEN
              LQ(KL) = LQ(LL-1)
              GO TO 22
            ELSE
              KL = LL - 1
              GO TO 22
            ENDIF
        ENDIF

      LDO = LQ(KDO)
      IF (LDO.EQ.0)                RETURN

      LQ(KL) = LQGARB
      LQGARB = LDO

      LQ(KDO) = 0
      RETURN
      END
