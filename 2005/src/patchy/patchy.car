      MU_INH  = IAND (ISHFTR(MU_DECK,5),  KM6-1)
      MX_TRAN = IAND (ISHFTR(MU_DECK,10), KM5-1)
      MX_FORG = IAND (MU_FORG, KM5-1)

      MXOR  =  IOR (MX_FORC,MU_DECK)
      MXAND = IAND (MX_FORC,MU_DECK)

