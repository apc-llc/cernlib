LIBDIRS += z268
#endif

#if defined(CERNLIB_HPUX)
LIBDIRS += t002
#endif

#if defined(CERNLIB_WINNT)
LIBDIRS += z264
#endif

SUBDIRS= $(LIBDIRS) kernbit test

TopOfPackage(kernbit)

SubdirLibraryTarget(kernbit,$(LIBDIRS))

TestSubdirs(test)

InstallIncludeSubdirs(kernbit)
