/* turn off optimisation */
CDEBUGFLAGS=
#endif

#ifdef CERNLIB_WINNT
/* turn off optimisation */
CDEBUGFLAGS = NoOpCDebugFlags
#endif

#if defined(CERNLIB_VAXVMS) && !defined(CERNLIB_QMALPH)
SpecialCObjectRule(NullParameter,qp_execute,qp_execute,/noopt)
#endif

CCOPTIONS := $(CCOPTIONS) CcAnsiMode

