#endif

#if defined(CERNLIB_LINUX)
EXTRA_DEFINES := $(EXTRA_DEFINES) -Df2cFortran
#endif

#if defined(CERNLIB_HPUX)
CCOPTIONS += -Wp,-H10000
#endif

#ifndef CERNLIB_VAXVMS
EXTRA_INCLUDES := $(EXTRA_INCLUDES) -I$(VPATH)
#endif

