install.lib::  $(foreach dir,$(LIBDIRS),$(dir)/Makefile)

install.lib::
	@set -e; \
	case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(LIBDIRS) ;\
	do \
		(cd $$i ; echo "installing" "in $(CURRENT_DIR)/$$i..."; \
			$(MAKE) $(MFLAGS) DESTDIR=$(DESTDIR) install.lib)||exit $$?; \
	done

